{
    "name": "Speed Dial Generator",
    "version": "1.2",
    "description": "Make Vivaldi automatically generate a cool SD.",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png"
    },
    "content_scripts": [ {
        "js":  [ "VivaldiSDgen.js" ],
        "matches": [ "<all_urls>" ],
        "run_at":  "document_start"
    } ],
    "manifest_version": 2
}
